{
  "name": "doc-agent",
  "version": "1.0.0",
  "description": "AI-powered documentation maintenance agent",
  "main": "dist/index.js",
  "bin": {
    "doc-agent": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "documentation",
    "ai",
    "agent",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "chalk": "^5.6.2",
    "commander": "^11.1.0",
    "diff": "^8.0.2",
    "prompts": "^2.4.2",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.28.0",
    "@types/diff": "^7.0.2",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tmp": "^0.2.5",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
